documento=open("config_datas.txt","r")
lines=documento.readlines()
lista=[]
for l in lines:
    parola=l.split(" ")
    prec=""
    for p in parola:
        if prec=="host":
            lista.append(prec , p)
        if prec=="user":
            lista.append(prec , p)
        if prec=="passwd":
            self.setPasswd(k)
        if prec=="db":
            self.setDB(k)